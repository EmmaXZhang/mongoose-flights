<%- include('../partials/header') %>
<h2>Enter New Ticket</h2>
<form action="/flights/<%=flight._id%>/tickets" method="POST">
  <label
    >Seat (A1-F99):<input
      type="string"
      placeholder="A1-F99"
      name="seat"
      required
      pattern="[A-F][1-9]\d?"
    /> </label
  ><br />
  <label>Price:<input type="number" name="price" /> </label><br />

  <button type="submit">Add Ticket</button>
</form>
